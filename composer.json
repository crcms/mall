{
    "name": "crcms/mall",
    "description": "crcms mall module",
	"keywords": ["category", "crcms","laravel","php"],
	"homepage": "https://github.com/crcms/mall",
    "license": "MIT",
    "type": "crcms-module",
    "require": {
        "php":">=7.2",
        "kalnoy/nestedset": "^4.3"
    },
	"autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "CrCms\\Mall\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CrCms\\Mall\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CrCms\\Mall\\Providers\\MallServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
